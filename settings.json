{
    "editor.fontSize": 15,
    "terminal.integrated.fontFamily": "Meslo LG S DZ for Powerline",
    "terminal.integrated.fontSize": 14,
    "workbench.colorTheme": "Solarized Dark",
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.rulers": [98],
    "editor.tokenColorCustomizations": {
        "[Solarized Dark]": {
            "textMateRules": [
                {
                    "scope": "variable.other.readwrite",
                    "settings": {
                        "foreground": "#7dcfcd",
                    }
                },
                {
                    "scope": "entity.name.type.module.elixir",
                    "settings": {
                        "foreground": "#268BD2",
                    }
                },
                {
                    "scope": "entity.name.function.call",
                    "settings": {
                        "foreground": "#a8b9bc",
                    }
                },
                {
                    "scope": "comment.documentation.heredoc.elixir",
                    "settings": {
                        "foreground": "#2ca028"
                    }
                },
                {
                    "scope": ["variable.other.readwrite.module.elixir", "punctuation.definition.variable", "variable.other.constant"],
                    "settings": {
                        "foreground": "#ddc448"
                    }
                },
                {
                    "scope": "keyword.control.elixir",
                    "settings": {
                        "fontStyle": "bold"
                    }
                },
                {
                    "scope": "meta.embedded",
                    "settings": {
                        "foreground": "#3befe0"
                    }
                },
                {
                    "scope": ["punctuation.definition.map.elixir", "punctuation.section.scope.elixir"],
                    "settings": {
                        "foreground": "#91db37"
                    }
                },
                {
                    "scope": "punctuation.section.embedded.elixir",
                    "settings": {
                        "foreground": "#4dbeff"
                    }
                },
                {
                    "scope": "punctuation.section.array.elixir",
                    "settings": {
                        "foreground": "#9682e4"
                    }
                },
                {
                    "scope": "punctuation.section.function.elixir",
                    "settings": {
                        "foreground": "#4dbeff"
                    }
                },
                {
                    "scope": "constant.numeric.elixir",
                    "settings": {
                        "foreground": "#df61d5",
                    }
                },
            ]
        },
    },
    "workbench.colorCustomizations": {
        "[Solarized Dark]": {
            "editorBracketHighlight.foreground1": "#4dbeff",
            "editorBracketHighlight.foreground2": "#e4e411",
            "editorBracketHighlight.foreground3": "#bd8aff"
        }
    },
    "editor.lineHeight": 1.6,
    "workbench.editorAssociations": {
        "*.ms": "hexEditor.hexedit"
    },
    "editor.bracketPairColorization.enabled": true,
    "editor.language.colorizedBracketPairs": [
        ["(", ")"]
    ],
    "files.autoSaveDelay": 300,
    "terminal.integrated.scrollback": 4000,
    "elixir.credo.lintOnSave": false,
    "security.workspace.trust.untrustedFiles": "open",
    "circleci.hostUrl": "",
    "circleci.ssh.privateKey.github": "/Users/jakubpryc/.ssh/id_ed25519",
    "window.nativeTabs": true,
    "workbench.startupEditor": "none",
    "circleci.ssh.terminalOrRemoteWindow": "In a remote SSH window",
    "editor.lineNumbers": "relative",
    "git.openRepositoryInParentFolders": "never",
    "editor.formatOnSave": true
}